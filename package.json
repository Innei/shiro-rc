{
  "name": "shiro-rc",
  "version": "0.0.4",
  "description": "TODO",
  "author": "Innei",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/innei/shiro-rc"
  },
  "type": "module",
  "files": [
    "dist",
    "readme.md",
    "tsconfig.json",
    "src"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./*": [
      "./*",
      "./*.d.ts"
    ]
  },
  "publishConfig": {},
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --ignore-path ./.prettierignore --write ",
      "eslint --cache"
    ]
  },
  "bump": {
    "before": [
      "npm run build"
    ],
    "publish": true,
    "changelog": true
  },
  "scripts": {
    "build": "vite build && tailwindcss -i src/tw.css -o dist/index.css",
    "prepare": "husky install",
    "release": "bump",
    "dev": "vite build --watch"
  },
  "devDependencies": {
    "@iconify-json/material-symbols": "1.1.74",
    "@iconify-json/mingcute": "1.1.16",
    "@iconify/tailwind": "0.1.4",
    "@innei/bump-version": "1.5.10",
    "@innei/eslint-config-react-ts": "0.12.5",
    "@innei/prettier": "0.12.4",
    "@tailwindcss/container-queries": "0.1.1",
    "@tailwindcss/typography": "0.5.10",
    "@types/node": "20.11.28",
    "@types/react": "18.2.66",
    "@vitejs/plugin-react": "4.2.1",
    "husky": "9.0.11",
    "lint-staged": "15.2.2",
    "postcss": "8.4.35",
    "postcss-import": "16.0.1",
    "postcss-nested": "6.0.1",
    "prettier": "3.2.5",
    "react": "18.2.0",
    "rollup-plugin-preserve-directives": "0.4.0",
    "tailwind-scrollbar": "3.1.0",
    "tailwindcss": "3.4.1",
    "tslib": "2.6.2",
    "typescript": "5.4.2",
    "vite": "5.1.6",
    "vite-plugin-dts": "3.7.3",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "1.4.0"
  },
  "dependencies": {
    "@floating-ui/react-dom": "2.0.8",
    "@radix-ui/react-scroll-area": "1.0.5",
    "@shikijs/transformers": "1.2.0",
    "clsx": "^2.1.0",
    "daisyui": "4.7.2",
    "dayjs": "1.11.10",
    "framer-motion": "^11.0.8",
    "jotai": "2.7.1",
    "shiki": "1.2.0",
    "tailwind-merge": "^2.2.1",
    "tailwind-variants": "0.2.0"
  }
}